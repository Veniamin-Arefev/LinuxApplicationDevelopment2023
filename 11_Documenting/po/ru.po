#: ../src/guessr.c:58
#, c-format
msgid "This is simple program that can guess your hidden number with a log(max number) tries\n"
msgstr "Это простая программа, которая может угадать ваш загаданный номер с помощью log(максимальное число) попыток\n"

#: ../src/guessr.c:62
#, c-format
msgid "Guess your integer between %s and %s\n"
msgstr "Загадайте целое число между %s и %s\n"

#: ../src/guessr.c:64
#, c-format
msgid "Answer on next questions only '%s' or '%s'\n"
msgstr "Отвечайте на следующие вопросы только '%s' либо '%s'\n"

#: ../src/guessr.c:64 ../src/guessr.c:71
msgid "yes"
msgstr "да"

#: ../src/guessr.c:64 ../src/guessr.c:73
msgid "no"
msgstr "нет"

#: ../src/guessr.c:69
#, c-format
msgid "Is your number greater than %s?\n"
msgstr "Ваше число больше чем %s?\n"

#: ../src/guessr.c:77
#, c-format
msgid "Your number is %s.\n"
msgstr "Ваше число - %s.\n"
